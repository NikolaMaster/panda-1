@model PandaWebApp.ViewModels.PhoneUnit
@{
    var countryCodeValues = (IEnumerable<SelectListItem>)ViewBag.CountryCodeValues;
}


@Html.DropDownListFor(x => x.CountryCode, countryCodeValues, new { @class="editor-field-country-code"})
@Html.TextBoxFor(x => x.Code, new { @class="editor-field-code"})
@Html.TextBoxFor(x => x.Number, new { @class="editor-field-number"}) 
