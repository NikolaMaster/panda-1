@{
    Layout = null;
}

<html>
	<head>
	    <title>@ViewBag.Title</title>
	    <meta charset="utf-8" />
	    <meta name="keywords"  content="@ViewBag.Keywords" />
	    <meta name="description" content="@ViewBag.Description" />
		
        <link rel="stylesheet" href="@Url.Content("~/Content/css/frameworks/jquery/south-street/jquery-ui-1.10.3.custom.min.css")" />
	    <link rel="stylesheet" href="@Url.Content("~/Content/css/style.css")" />
	</head>
	
	<body>
	
		<div id="top" class="top-panel layer zero"></div>
		
		<div class="body layer one">
			<div class="header">
				<div class="top-links">
					<a href="#" class="left middle">Выберите город</a>
					<a href="#" class="right middle-img"><img src="/Content/img/enter-button.png" /></a>
					<a href="#" class="right middle">Зарегистрироваться на сайте</a>
					
					<div class="clear"></div>
				</div>
				
				<div class="logo left"><img src="/Content/img/logo.png" /></div>
				<div class="right">
					<div class="banner-top"></div>
					<ul class="main-menu horiz">
						<li><a href="#">О проекте</a></li>
						<li><a href="#">Словарь</a></li>
						<li><a href="#">Блог</a></li>
						<li><a href="#">Вопрос-Ответ</a></li>
						<li><a href="#">Контакты</a></li>
						<li><a href="#">Платные услуги</a></li>
					</ul>
				</div>
				<div class="clear"></div>
				
				<div id="shadow" class="top-shadow layer zero"></div>
			</div>
			<div class="content">
				<div class="right-sidebar right">
					<div class="pulse">
						<div class="title">Панда пульс</div>
						<ul>
							<li class="temp"></li>
							<li class="header">
								<span class="left">ОНЛАЙН</span>
								<span class="right"><a href="#">#TOTALUSERS#</a></span>
								<div class="clear"></div>
							</li>
							<li class="first">
								<span class="icon"><img src="" /></span>
								<span class="name"><a href="#">#USERNAME#</a></span>
							</li>
							<li>
								<span class="icon"><img src="" /></span>
								<span class="name"><a href="#">#USERNAME#</a></span>
							</li>
							<li>
								<span class="icon"><img src="" /></span>
								<span class="name"><a href="#">#USERNAME#</a></span>
							</li>
							<li>
								<span class="icon"><img src="" /></span>
								<span class="name"><a href="#">#USERNAME#</a></span>
							</li>
							<li>
								<span class="icon"><img src="" /></span>
								<span class="name"><a href="#">#USERNAME#</a></span>
							</li>
							<li>
								<span class="icon"><img src="" /></span>
								<span class="name"><a href="#">#USERNAME#</a></span>
							</li>
							<li>
								<span class="icon"><img src="" /></span>
								<span class="name"><a href="#">#USERNAME#</a></span>
							</li>
							<li>
								<span class="icon"><img src="" /></span>
								<span class="name"><a href="#">#USERNAME#</a></span>
							</li>
						</ul>
						<img src="/Content/img/pulse-bottom.png" />
					</div>
					
					<div class="banner-side empty"></div>
					<div class="banner-side empty"></div>
					<div class="banner-side empty"></div>
					<div class="banner-side empty"></div>
					<div class="banner-side empty"></div>
				</div>
                @RenderBody()
				<div class="clear"></div>
			</div>
			<div class="footer"></div>
		</div>
	
        <!-- include scripts there -->
        <script type="text/javascript" src="@Url.Content("~/Content/js/frameworks/jquery/jquery-1.9.1.js")"></script>
        <script type="text/javascript" src="@Url.Content("~/Content/js/frameworks/jquery/jquery-ui-1.10.3.custom.min.js")"></script>
	</body>
</html>
